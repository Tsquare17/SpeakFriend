<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" stylesheets="@css/materialfx.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER" VBox.vgrow="NEVER">
            <children>
                <Label fx:id="message" text="Confirm" />
            </children>
            <padding>
                <Insets bottom="30.0" top="30.0" right="30" left="30" />
            </padding>
        </HBox>
        <GridPane vgap="5.0" hgap="15.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button mnemonicParsing="false" fx:id="cancel" text="Cancel" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.halignment="RIGHT" />
                <Button mnemonicParsing="false" fx:id="confirm" text="Confirm" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.halignment="LEFT" />
            </children>
            <padding>
                <Insets left="10.0" right="10.0" />
            </padding>
        </GridPane>
    </children>
</VBox>
